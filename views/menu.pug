extends layout

block content
    include ./component/resultSearch
    div(class='newShareDiv')
        table(class="ui celled table" id='newShareList')
            thead
                tr
                    th(class='ten wide') 文件名称
                    th(class='two wide center aligned')  类型
                    th(class='two wide center aligned')  大小
            tbody
                each result in results
                    tr
                        td
                            - var title = result.title;
                            a(class='text' href=`/result/?view=${result.ID}&kw=${result.title}` target='_blank')
                                !=  title
                        td(class='center aligned') #{result.category}
                        td(class='center aligned') #{result.size}
            tfoot
                tr
                    th(colspan="3")
                        div(class="ui floated pagination menu" id='menu-pages')
                            a(class="icon item" data-value='1' href=`/menu?type=${results.typeID}&p=1`)
                                span 1
                            a(class="icon item" data-value='2' href=`/menu?type=${results.typeID}&p=2`)
                                span 2
                            a(class="icon item" data-value='3' href=`/menu?type=${results.typeID}&p=3`)
                                span 3
                            a(class="icon item" data-value='4' href=`/menu?type=${results.typeID}&?p=4`)
                                span 4
                            a(class="icon item" data-value='5' href=`/menu?type=${results.typeID}&p=5`)
                                span 5
                            a(class="icon item" data-value='6' href=`/menu?type=${results.typeID}&p=6`)
                                span 6
