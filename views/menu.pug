                                span 7
                            a(class="icon item" data-value='8' href=`/menu?type=${results.typeID}&p=8`)
                                span 8
                            a(class="icon item" data-value='9' href=`/menu?type=${results.typeID}&p=9`)
                                span 9
                            a(class="icon item" data-value='10' href=`/menu?type=${results.typeID}&p=10`)
